{
  "name": "dotup-vscode-interface-generator",
  "displayName": "Interface generator",
  "description": "Create interface from class",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.31.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.dotupGenerateInterface"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.dotupGenerateInterface",
        "title": "Generate interface"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "typescript-parser": "^2.6.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "ts-node": "^8.0.2",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "tslint-microsoft-contrib": "^6.1.0"
  }
}
